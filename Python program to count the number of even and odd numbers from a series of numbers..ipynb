    "a=[]\n",
    "size=int(input(\"how many elements you want to enter\"))\n",
    "for i in range(size):\n",
    "    val=int(input(\"enter number:\"))\n",
    "    a.append(val)\n",
    "even=0\n",
    "odd=0\n",
    "for i in range(size):\n",
    "    if(a[i]%2==0):\n",
    "        even=even+1\n",
    "    else:\n",
    "        odd=odd+1\n",
    "print(\"total even=\",even,\"total odd=\",odd)"
